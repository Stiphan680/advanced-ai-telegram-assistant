# Telegram Bot Token
# Aapko BotFather se ye token milega
# Telegram mein /botfather ko message do aur naya bot banao
TELEGRAM_BOT_TOKEN=your_bot_token_here

# Claude API Key
# Anthropic console se ye key generate karo
# https://console.anthropic.com/
CLAUDE_API_KEY=your_claude_api_key_here

# Telegram Channel ID (Optional)
# Updates ke liye - agar channel pe notifications bhejne hain
CHANNEL_ID=your_channel_id_here

# Local Development Port
PORT=8000

# Environment Type
ENVIRONMENT=development
